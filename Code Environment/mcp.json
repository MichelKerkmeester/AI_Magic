{
  "mcpServers": {
    "code_mode": {
      "command": "node",
      "args": [
        "/path/to/code-mode-mcp/dist/index.js"
      ],
      "env": {
        "UTCP_CONFIG_FILE": "/path/to/.utcp_config.json"
      },
      "working_directory": null,
      "disabled": false
    },
    "sequential-thinking": {
      "command": "/path/to/mcp-sequential-thinking/.venv/bin/python3.12",
      "args": [
        "-m",
        "mcp_sequential_thinking.server"
      ],
      "working_directory": null,
      "disabled": false
    }
  }
}